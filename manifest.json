{
    "manifest_version": 3,
    "name": "QR Code Generator",
    "version": "1.0",
    "description": "Generates a QR code for the current tab's URL",
    "icons": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    },
    "action": {
      "default_icon": "icon.png",
      "default_title": "QR Code Generator",
      "default_popup": "popup.html"
    },
    "permissions": [
      "activeTab"
    ]
  }
  